{
  "name": "intelligencia-genea-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "build:prod": "ng build --configuration production --output-path=dist",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:critical": "ng build --configuration production && critical src/index.html --base dist/ --inline --minify",
    "serve:ssr:intelligencia-genea-Angular": "node dist/intelligencia-genea-Angular/server/server.mjs",
    "lh": "npx lighthouse http://localhost:4200/ --view"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.2.4",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.2.4",
    "@angular/forms": "^20.2.4",
    "@angular/platform-browser": "^20.2.4",
    "@angular/platform-server": "^20.1.0",
    "@angular/router": "^20.2.4",
    "@angular/ssr": "^20.1.2",
    "@emailjs/browser": "^4.4.1",
    "@fortawesome/fontawesome-free": "^7.0.1",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.7",
    "express": "^5.1.0",
    "mongodb": "^6.19.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "web-vitals": "^5.1.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.2.2",
    "@angular/build": "^20.1.2",
    "@angular/cli": "^20.1.2",
    "@angular/compiler-cli": "^20.1.0",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/mongodb": "^4.0.6",
    "@types/node": "^20.17.19",
    "critical": "^7.2.1",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lighthouse": "^12.8.2",
    "sharp": "^0.34.3",
    "typescript": "~5.8.2"
  }
}
